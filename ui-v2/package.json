{
  "description": "One-page Web Application for the AWS Ops Wheel. README at https://github.com/aws/aws-ops-wheel",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-ops-wheel.git"
  },
  "scripts": {
    "test": "npm run coverage",
    "coverage": "nyc mocha --config .mocharc.json",
    "build": "npm run build-dev && npm run build-prod",
    "build-dev": "NODE_ENV=development webpack --progress --color --mode development",
    "build-prod": "NODE_ENV=production webpack --progress --color --mode production",
    "unit-test": "mocha --config .mocharc.json",
    "test-watch": "mocha --config .mocharc.json --watch",
    "setup-dev": "node setup-dev-environment.js",
    "dev": "npm run setup-dev && npm run start",
    "start": "webpack serve",
    "flow": "flow"
  },
  "nyc": {
    "include": [
      "src/*.jsx",
      "src/**/*.jsx"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "core-js/stage/4",
      "@babel/register",
      "source-map-support/register",
      "ignore-styles"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": false,
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "ajv-keywords": "^5.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "canvas": "^2.9.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "1.0.0-rc.12",
    "css-loader": "^6.8.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "file-loader": "^6.1.1",
    "flow-bin": "0.59.0",
    "flow-typed": "^4.1.1",
    "glob-parent": "^5.1.2",
    "history": "^4.7.2",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.3.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.7.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "minimist": "^1.2.6",
    "mocha": "^11.7.1",
    "nock": "^14.0.5",
    "nyc": "^15.1.0",
    "sinon": "^7.3.2",
    "source-map-loader": "^0.2.3",
    "ssri": ">=8.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.95.0",
    "webpack-dev-server": "^5.2.2",
    "y18n": ">=3.2.2"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^2.0.30",
    "bootstrap": "^5.3.0",
    "core-js": "^2.5.1",
    "deep-extend": "^0.5.1",
    "es6-promise": "^4.1.1",
    "extend": "^3.0.2",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^3.0.0",
    "just-extend": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "node-fetch": ">=3.2.10",
    "nth-check": "^2.1.1",
    "pixi.js": "^4.6.2",
    "randomatic": "^3.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-redux": "^5.0.6",
    "react-redux-fetch": "^0.15.0",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sshpk": "^1.13.2",
    "url": "^0.11.0",
    "url-parse": "^1.5.8",
    "validate.js": "^0.12.0",
    "webpack-cli": "^6.0.1",
    "yargs-parser": "^20.2.7"
  },
  "name": "ui",
  "version": "1.0.0",
  "resolutions": {
    "cheerio": "1.0.0-rc.12",
    "@octokit/request": "8.4.1",
    "@octokit/plugin-paginate-rest": "9.2.2",
    "@octokit/request-error": "5.1.1"
  }
}
