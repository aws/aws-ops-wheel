
{
  "presets": [
    "@babel/preset-env", 
    "@babel/preset-react", 
    [
      "@babel/preset-typescript", 
      {
        "allowDeclareFields": true,
        "allowNamespaces": true,
        "allExtensions": true,
        "isTSX": true
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
