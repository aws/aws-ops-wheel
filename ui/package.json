{
  "description": "One-page Web Application for the AWS Ops Wheel. README at https://github.com/aws/aws-ops-wheel",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-ops-wheel.git"
  },
  "scripts": {
    "test": "npm run coverage",
    "coverage": "nyc mocha --opts mocha.opts",
    "build": "npm run build-dev && npm run build-prod",
    "build-dev": "NODE_ENV=development webpack --progress --color -d",
    "build-prod": "NODE_ENV=production webpack --progress --color -p",
    "unit-test": "mocha --opts mocha.opts",
    "test-watch": "mocha --opts mocha.opts --watch",
    "start": "webpack-dev-server --compress --host 0.0.0.0 --public --content-base ./ -d --devtool source-map --inline --hot",
    "flow": "flow"
  },
  "nyc": {
    "include": [
      "src/*.jsx",
      "src/**/*.jsx"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "core-js/stage/4",
      "babel-core/register",
      "source-map-support/register",
      "ignore-styles"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": false,
    "all": true
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "file-loader": "^3.0.1",
    "flow-bin": "0.59.0",
    "flow-typed": "^2.2.3",
    "history": "^4.7.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "nock": "^9.1.0",
    "nyc": "^11.3.0",
    "sinon": "^4.1.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.26.0",
    "bootstrap": "^3.4.1",
    "core-js": "^2.5.1",
    "es6-promise": "^4.1.1",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.19.3",
    "moment-timezone": "^0.5.14",
    "pixi.js": "^4.6.2",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "16",
    "react-redux": "^5.0.6",
    "react-redux-fetch": "^0.11.1",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "validate.js": "^0.12.0",
    "extend": "^3.0.2",
    "just-extend": "^4.0.0",
    "randomatic": "^3.0.0",
    "deep-extend": "^0.5.1",
    "sshpk": "^1.13.2",
    "url-parse": "^1.4.3",
    "lodash": "^4.17.11",
    "hoek": "^4.2.1"
  }
}
